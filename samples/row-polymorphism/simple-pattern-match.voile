let Unit = Rec {};

val unit : Unit;
let unit = {| |};

let Bool = Sum { True: Unit; False: Unit; };

val true : Bool;
let true = @True unit;

val false : Bool;
let false = @False unit;

val not : Bool -> Bool;
let not = case True u: false
  or case False u: true
  or whatever;
